{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "vite:dev",
      "type": "shell",
      "command": "npm run dev",
      "isBackground": true,
      "problemMatcher": {
        "owner": "custom",
        "fileLocation": "absolute",
        "pattern": [
          {
            "regexp": ".",
            "file": 1,
            "location": 2,
            "message": 3
          }
        ],
        "background": {
          "activeOnStart": true,
          "beginsPattern": "Local:\\s*http",
          "endsPattern": "ready in"
        }
      }
    }
  ]
}